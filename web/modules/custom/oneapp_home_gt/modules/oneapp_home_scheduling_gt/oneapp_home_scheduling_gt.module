  $form[$group]['visit_status']['list_add_row'] = [
    '#type' => 'submit',
    '#value' => t('Agregar Estado'),
    '#submit' => ['addRowStatus'],
    '#ajax' => [
      'callback' => 'rowStatusCallback',
      'wrapper' => 'status-wrapper',
    ]
