      '#type' => 'weight',
      '#title' => t('Weight status'),
      '#title_display' => 'invisible',
      '#default_value' => isset($items[$i]['weight']) ? $items[$i]['weight'] : $i,
      '#attributes' => ['class' => ['fields-order-weight-fields']],
    ];
