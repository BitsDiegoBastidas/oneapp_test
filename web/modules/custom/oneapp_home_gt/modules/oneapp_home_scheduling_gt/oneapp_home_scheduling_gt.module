  $form[$group]['visit_cancel_form']['success']['mail']['body'] = [
    '#type' => 'text_format',
    '#title' => t('Cuerpo'),
    '#format' => 'full_html',
    '#default_value' => isset($config['visit_cancel_form']['success']['mail']['body']['value']) ?
